{
    "name": "extism/extism",
    "description": "Make your software programmable. Run WebAssembly extensions in your app using the first off-the-shelf, universal plug-in system.",
    "license": "BSD-3-Clause",
    "type": "library",
    "keywords": [
        "WebAssembly",
        "plugin-system",
        "runtime",
        "plug-in"
    ],
    "authors": [
        {
            "name": "The Extism Authors",
            "email": "oss@extism.org",
            "homepage": "https://extism.org"
        },
        {
            "name": "Dylibso, Inc.",
            "email": "oss@dylib.so",
            "homepage": "https://dylib.so"
        }
    ],
    "require": {
        "php": "^7.4 || ^8",
        "ircmaxell/ffime": "dev-master"
    },
    "suggest": {},
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Extism\\": "php/src/"
        },
        "files": [
            "php/src/Plugin.php",
            "php/src/generate.php",
            "php/src/extism.h"
        ]
    },
    "autoload-dev": {
        "psr-4": {}
    },
    "config": {
        "sort-packages": true
    },
    "extra": {},
    "scripts": {},
    "scripts-descriptions": {}
}